{
  "name": "stickwhack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/styles": "^4.11.5",
    "jquery": "^3.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "sass": "^1.43.4",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "s3-deploy:dev": "aws s3 sync build/ s3://dev-stickwhack",
    "cloudfront-invalidation:dev": "aws cloudfront create-invalidation --distribution-id ES98HQ4NNATUX --paths /*",
    "deploy:dev": "yarn build && yarn s3-deploy:dev && yarn cloudfront-invalidation:dev",
    "cloudfront-invalidation:prod": "aws cloudfront create-invalidation --distribution-id E17FATCAHMZIB7 --paths /*",
    "s3-deploy:prod": "aws s3 sync build/ s3://prod-stickwhack",
    "deploy:prod": "yarn build && yarn s3-deploy:prod && yarn cloudfront-invalidation:prod"
  },
  "pre-commit": [
    "precommit-msg",
    "format",
    "lint"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/node-logger": "^6.4.21",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/jquery": "^3.5.9",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "webpack": "^5.72.0"
  }
}
